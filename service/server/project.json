{
  "name": "vspo-portal-server",
  "targets": {
    "build-dryrun": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bash scripts/dry-run-all-workers.sh"
      }
    },
    "build-dryrun:vspo-portal-cron": {
      "executor": "nx:run-commands",
      "options": {
        "command": "wrangler deploy --config config/wrangler/dev/vspo-portal-cron/wrangler.jsonc --dry-run --script cmd/cron/index.ts"
      }
    },
    "build-dryrun:vspo-portal-gateway": {
      "executor": "nx:run-commands",
      "options": {
        "command": "wrangler deploy --config config/wrangler/dev/vspo-portal-gateway/wrangler.jsonc --dry-run --script cmd/server/gateway.ts"
      }
    },
    "build-dryrun:clip-analysis-query": {
